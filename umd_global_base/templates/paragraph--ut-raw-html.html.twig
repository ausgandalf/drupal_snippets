{#
/**
 * @file
 * Theme override to display a Raw HTML Embed Code Paragraph.
 */
#}

{%
  set classes = [
    'raw_html'
  ]
%}

{% block paragraph %}
	<div{{attributes.addClass(classes).setAttribute('id','umd_terp_paragraph--'~paragraph.id())}}>
		{% block content %}
			{{ content.field_ut_raw_html.0['#context']['value']|raw }}
		{% endblock %}
	</div>
{% endblock paragraph %}

