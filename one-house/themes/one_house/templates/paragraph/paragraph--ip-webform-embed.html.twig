{%
  set classes = [
    'paragraph-widget' ~ ((_page_type == 'homepage') ? '--home'),
    'paragraph-widget--webformviews-embed',
  ]
%}

{% block paragraph %}

  <div {{attributes.addClass(classes).setAttribute('id','ip_paragraph_adv-'~paragraph.id())}}>

    {% block content %}
      <div class="text-content">
        {{ content.field_ip_webform_embed_item }}
      </div>
    {% endblock %}

  </div>

{% endblock paragraph %}